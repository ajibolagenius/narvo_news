{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "narvo.live" }],
      "destination": "https://narvo.news/:path*",
      "permanent": true
    }
  ]
}
